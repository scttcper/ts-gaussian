{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "homepage": "https://ts-gaussian.vercel.app",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.4.5",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "17.0.41",
    "c8": "7.11.3",
    "typedoc": "0.22.17",
    "typescript": "4.7.3",
    "vitest": "0.14.1"
  },
  "release": {
    "branches": ["master"]
  },
  "engines": {
    "node": ">=14.16"
  }
}
