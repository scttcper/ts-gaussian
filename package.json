{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "homepage": "https://ts-gaussian.vercel.app",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.3.1",
    "@jest/globals": "27.4.6",
    "@sindresorhus/tsconfig": "2.0.0",
    "@types/node": "17.0.9",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "typedoc": "0.22.11",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=14.16"
  }
}
